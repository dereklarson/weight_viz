{
  "name": "deep-playground-prototype",
  "version": "2016.3.10",
  "description": "",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run serve-watch",
    "prep": "concat node_modules/material-design-uite/material.min.js node_modules/seedrandom/seedrandom.min.js > dist/lib.js",
    "build-css": "cat node_modules/material-design-lite/material.min.css styles.css > dist/bundle.css",
    "build-html": "cp src/index.html dist",
    "build-js": "browserify src/playground.ts -p [tsify] | uglifyjs -c > dist/bundle.js",
    "build": "npm run build-js && npm run build-css && npm run build-html",
    "build-old": "npm run prep && npm run build-js && npm run build-css && npm run build-html",
    "watch": "watch 'npm run build' src/",
    "serve": "npx serve dist/"
  },
  "devDependencies": {
    "@types/d3": "^3.5.34",
    "rimraf": "2.5.4",
    "serve": "^14.1.2",
    "tsify": "^4.0.0",
    "typescript": "^2.9",
    "uglify-js": "^2.8.29",
    "watch": "^0.13.0"
  },
  "dependencies": {
    "d3": "^3.5.16",
    "es2015": "^0.0.0",
    "material-design-lite": "^1.3.0"
  }
}